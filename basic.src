IMPORT "IO.bang"

Vector3 :: STRUCT
  x : F32
  y : F32
  z : F32

PrintVector :: (vector : @Vector3)
  Println(vector.x)
  Println(vector.y)
  Println(vector.z)

+ :: (a : @Vector3 b : @Vector3) :> Vector3
	vector : Vector3
	vector.x = a.x + b.x
	vector.y = a.y + b.y
	vector.Z = a.z + b.z
	RETURN vector

- :: (a : @Vector3 b : @Vector3) :> Vector3
	vector : Vector3
	vector.x = a.x - b.x
	vector.y = a.y + b.y
	vector.z = a.z + b.z
	RETURN vector
