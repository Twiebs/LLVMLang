IMPORT "IO.bang"

Vector3 :: STRUCT {
  x : S32
  y : S32
  z : S32
}

Transform :: STRUCT {
  position : Vector3
}

PrintVector :: (vector : @Vector3) {
  Println(vector.x)
  Println(vector.y)
  Println(vector.z)
}

Main :: () :> S32 {
  transform : Transform
  transform.position.x = 5
  PrintVector(@transform.position)

  RETURN 0
}
