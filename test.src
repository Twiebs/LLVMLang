import "IO.bang"

MemoryArena :: struct {
  used : S32
  capacity : S32
}

Worker :: struct {
  arena : MemoryArena
}

PrintArena :: (arena : @MemoryArena) {
  Println(arena.used)
  Println(arena.capacity)
}

Main :: () :> S32 {
  worker : Worker
  worker.arena.used = 2
  worker.arena.capacity = 4096

  PrintArena(@worker.arena)

  return 0
}
